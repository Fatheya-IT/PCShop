<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:gravity="center"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/s">
    <include
        layout="@layout/logo_header"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="wrap_content"/>

    <TextView
        android:id="@+id/homeTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About Us"
        android:textSize="36sp"
        android:textStyle="bold"
        android:paddingTop="100dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="100dp"
        android:textStyle="bold"
        android:text="DEV TEAM:\n- Ranim Albogami \n- Wed Alnrweshid \n- Fatheya Baljoon \n- Alanoud Alrasheedi \n Degree:\n- IT\nTECH:\n- Android Studio, Java, XML"
        android:textSize="20dp" />

    <include
        android:id="@+id/top_nav"
        layout="@layout/top_nav"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</LinearLayout>
